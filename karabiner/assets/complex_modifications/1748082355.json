{
  "title": "Fn + [ to Esc",
  "rules": [
    {
      "description": "Change Fn + [ to Esc",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
